@model IEnumerable<CDMS.Entities.DSTRCT>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @*@Html.ActionLink("Create New", "Create")*@
    <a href="~/Dstrcts/Create" class="btn btn-default btn-sm">Create New</a>

    <a href="~/D/Create" class="btn btn-default btn-sm">Add DS</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.districtname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ADA.adaname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.districtname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ADA.adaname)
        </td>
        <td>
            @Html.ActionLink("Modify", "Edit", new { id=item.districtid }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.districtid }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.districtid })
        </td>
    </tr>
}

</table>
