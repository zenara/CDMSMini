@model IEnumerable<CDMS.Entities.ASC>

@{
    ViewBag.Title = "Index";
}

<h2>Agrarian Service Centers</h2>

<p>
    @*@Html.ActionLink("Create New", "Create")*@
    <a href="~/ASCs/Create" class="btn btn-default btn-sm">Create New</a>

    <a href="~/AILISTs/Create" class="btn btn-default btn-sm">Add AI</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.D.dsname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ascname)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.D.dsname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ascname)
        </td>
        <td>
            @Html.ActionLink("Modify", "Edit", new { id=item.ascid }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.ascid }) |*@
            @Html.ActionLink("Delete", "Delete", new { id=item.ascid })
        </td>
    </tr>
}

</table>
